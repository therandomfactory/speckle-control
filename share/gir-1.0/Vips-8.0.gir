|[
VipsObject **t;

t = vips_object_local_array( a, 5 );
if(
  vips_add( a, b, &amp;amp;t[0], NULL ) ||
  vips_invert( t[0], &amp;amp;t[1], NULL ) ||
  vips_add( t[1], t[0], &amp;amp;t[2], NULL ) ||
  vips_costra( t[2], out, NULL ) )
  return( -1 );
]|

